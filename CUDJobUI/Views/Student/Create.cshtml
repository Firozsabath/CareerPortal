    <div class="col-xl-12">
        <form asp-action="Create" asp-controller="Student" method="post" enctype="multipart/form-data" id="StudentDetails" class="needs-validation" novalidate>
            @*<form id="StudentDetails">*@
            <div class="tab-content" id="pills-tabContent">
                <div class="tab-pane fade show active" id="pills-Personal" role="tabpanel" aria-labelledby="pills-Personal-tab">
                    <div class="card mb-3">
                        <div class="card-header" style="color: #d5202a;font-weight:bold;">
                            Student Personal Information
                        </div>
                        <div class="card-body">
                            <div class="outer_div mb-3">
                                <fieldset>
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentPersonal.FirstName" class="control-label"></label>
                                                <input asp-for="StudentPersonal.FirstName" class="form-control" required readonly />
                                                <div class="invalid-feedback">Please provide a First Name.</div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentPersonal.LastName" class="control-label"></label>
                                                <input asp-for="StudentPersonal.LastName" class="form-control" required readonly />
                                                <div class="invalid-feedback">Please provide a Last Name.</div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="form-group">

                                                <label asp-for="StudentPersonal.EmailID" class="control-label"></label>
                                                <input asp-for="StudentPersonal.EmailID" class="form-control" required readonly />
                                                <div class="invalid-feedback">Email ID Number is Required.</div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentPersonal.DateOfBirth" class="control-label"></label>
                                                <input type="date" asp-for="StudentPersonal.DateOfBirth" class="form-control" required readonly />
