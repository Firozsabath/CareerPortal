                                        <div class="invalid-feedback">Skillsets is Required.</div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="StudentPersonal.SskillID" class="control-label"></label>
                                        <span class="d-inline-block" tabindex="0" data-toggle="tooltip"
                                              title="Soft skills are qualities that enable a person to interact effectively and amicably with other people.">
                                            <i class="fa fa-question-circle" aria-hidden="true"></i>
                                        </span>
                                        <select asp-for="Sskills" class="multi_select w-100" asp-items="@(new SelectList(ViewBag.Sskills, "SskillID", "SoftSkills"))" multiple></select>
                                        @*<textarea asp-for="StudentPersonal.Skillset" class="form-control rssInstitution mytextarea" required></textarea>*@
                                        <div class="invalid-feedback">Skillsets is Required.</div>
                                    </div>
                                </div>

                                <!--<div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="StudentPersonal.CskillID" class="control-label"></label>
                                        <span class="d-inline-block" tabindex="0" data-toggle="tooltip"
                                              title="Soft skills are qualities that enable a person to interact effectively and amicably with other people.">
                                            <i class="fa fa-question-circle" aria-hidden="true"></i>
                                        </span>
                                        <select asp-for="Cskills" class="multi_select w-100" asp-items="@(new SelectList(ViewBag.Cskills, "CskillID", "SoftSkills"))" multiple></select>-->
                                @*<textarea asp-for="StudentPersonal.Skillset" class="form-control rssInstitution mytextarea" required></textarea>*@
                                <!--<div class="invalid-feedback">Skillsets is Required.</div>
                                    </div>
                                </div>-->
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label asp-for="StudentPersonal.CskillID" class="control-label"></label>
                                        @*<select asp-for="Cskills" class="form-control" asp-items="@(new SelectList(ViewBag.Cskills, "CskillID", "SoftSkills"))"></select>*@
                                        <textarea asp-for="Cskills" class="form-control rssInstitution mytextarea"></textarea>
                                        <div class="invalid-feedback">Skillsets is Required.</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card mb-3">
                        <div class="card-header" style="color: #d5202a;font-weight:bold;">
                            <div class="row">
                                <div class="col-md-3">
                                    Education
                                </div>
                                <div class="col-md-9">
                                    <p><button type="button" class="btn btn-info float-right" id="Btn_Eduadd">Add</button></p>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="Education_wrapper rssRow outer_div mb-3">
                                <span class="removeEducation" style="display:none;"><i class="fa fa-times" style="color:red;cursor:pointer;">x Remove</i></span>
                                <fieldset>
                                    <div class="row">
                                        <div class="col-lg-12 currentedudv">
                                            <div class="form-group">
                                                <div class="form-check form-switch ml-2">
                                                    <input class="form-check-input currentlyedu" type="checkbox" asp-for="StudentEducation[0].current">
                                                    <label class="form-check-label" asp-for="StudentEducation[0].current">Current Education</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentEducation[0].Institution" class="control-label"></label>
                                                <input asp-for="StudentEducation[0].Institution" data-val="true" class="form-control rssInstitution" />
                                                <div class="invalid-feedback">Institution is Required.</div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentEducation[0].Degree" class="control-label">Degree <span class="text-danger">*</span></label>
                                                <input asp-for="StudentEducation[0].Degree" data-val="true" class="form-control rssDegree" required />
                                                <div class="invalid-feedback">Degree is Required.</div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentEducation[0].Major" class="control-label">Major <span class="text-danger">*</span></label>
                                                <input asp-for="StudentEducation[0].Major" data-val="true" class="form-control rssMajor" required />
                                                <div class="invalid-feedback">Major is Required.</div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-4" style="display:none;">
                                            <div class="form-group">
                                                <label asp-for="StudentEducation[0].CompletionPercent" class="control-label"></label>
                                                <input asp-for="StudentEducation[0].CompletionPercent" class="form-control rssMajor" />
                                                <span asp-validation-for="StudentEducation[0].CompletionPercent" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentEducation[0].StartDate" class="control-label"></label>
                                                <input type="date" asp-for="StudentEducation[0].StartDate" class="form-control rssEduStartdate" />
                                                <span asp-validation-for="StudentEducation[0].StartDate" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label asp-for="StudentEducation[0].CompletionDate" class="control-label"></label>
                                                <input type="date" asp-for="StudentEducation[0].CompletionDate" class="form-control rssEduEnddate" />
                                                <span asp-validation-for="StudentEducation[0].CompletionDate" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">

                                        @*<div class="col-lg-6">
                                                <div class="form-group">
                                                    <label asp-for="StudentDegree.CertificateDiploma" class="control-label"></label>
                                                    <input type="date" asp-for="StudentDegree.CertificateDiploma" class="form-control" />
                                                    <span asp-validation-for="StudentDegree.CertificateDiploma" class="text-danger"></span>
                                                </div>
                                            </div>*@
                                    </div>
                                </fieldset>
                            </div>
                            <div class="Education_WrapperDynamic"></div>
                        </div>
                    </div>
                    <div class="card mb-3">
                        <div class="card-header" style="color: #d5202a;font-weight:bold;">
                            Summary <span class="d-inline-block" tabindex="0" data-toggle="tooltip" title="Write a brief Objective (if no experience) or a Profile statement (if you have experience) - in 1 or 4 lines - at the top of your resume to summarize and highlight relevant  Skills/Professional or Volunteering Experiences/Accomplishments/Knowledge/Education. Show the employer, at a glance, why you’re qualiﬁed for the job and what do you want from them.

                                         Example of Objective: Motivated high school student experienced in serving others. Has proven customer service, organization, and communication skills. Familiar with best practices for serving and willing to learn.

                                         Example of Profile: Honor roll high school student with an aptitude for customer service. Has experience working for retailers, such as Old Navy and Gap. Experienced using a variety of point of sale systems and price checkers. Eager to develop management skills.">
                                <i class="fa fa-question-circle" aria-hidden="true"></i>
                            </span>
                        </div>
                        <div class="card-body">
                            <div class="mb-3">
                                <div class="form-group">
                                    <label asp-for="StudentPersonal.Objective" class="control-label">Write your summary <span class="text-danger">*</span></label>                                   
                                    @*<textarea asp-for="StudentPersonal.Objective" class="form-control" required></textarea>*@
                                    <textarea asp-for="StudentPersonal.Objective" class="form-control" maxlength="150" rows="4" required></textarea>
                                    <div class="invalid-feedback">Please provide an Objective.</div>
                                </div>
                            </div>
                        </div>
                        <div class="card-footer">
                            <div class="row">
                                <div class="col-lg-10"></div>
                                <div class="col-lg-2">
                                    <button class="btn btn-danger btn-block" id="Edunext" type="button">Next</button>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="tab-pane fade" id="pills-Experience" role="tabpanel" aria-labelledby="pills-Experience-tab">
                    <div class="card mb-3">
                        <div class="card-header" style="color: #d5202a;font-weight:bold;">
                            Upload Your Resume
                        </div>
                        <div class="card-body">
                            <div class="mb-3">
                                @*<label for="formFile" class="form-label">Upload Your Resume</label>*@
                                <input type="file" class="form-control-file" name="file">
                            </div>
                        </div>
                    </div>

                    <div class="card mb-3">
                        <div class="card-header" style="color: #d5202a;font-weight:bold;">
                            <div class="row">
                                <div class="col-lg-3">
                                    <p>
                                        Volunteering Experience
                                        <span class="d-inline-block" tabindex="0" data-toggle="tooltip" title="Read the jobs that you are interested in and make a list of all of the skills listed in the descriptions.Identify the skills that you possess.
                                                                                    Add those skills to the Skills section of your resume and illustrate them in the Work experience section of your resume by attaching them to certain accomplishments or responsibilities.">
                                            <i class="fa fa-question-circle" aria-hidden="true"></i>
                                        </span>
                                    </p>
                                </div>
                                <div class="col-lg-9">
                                    <p><button type="button" class="btn btn-info float-right" id="Btn_Vexpadd">Add</button></p>
                                </div>
                            </div>
                        </div>
                        <div class="card-body" id="StdVExperience">
